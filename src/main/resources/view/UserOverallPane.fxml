<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" VBox.vgrow="SOMETIMES" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" styleClass="pane-with-border" minWidth="340">
   <children>
      <HBox fx:id="userProfilePlaceholder" styleClass="pane-with-border">
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <VBox fx:id="userDetailPlaceholder" styleClass="pane-with-border" VBox.vgrow="ALWAYS">
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</VBox>
